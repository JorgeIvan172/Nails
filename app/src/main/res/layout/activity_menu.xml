<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Menu">

    <TextView
        android:id="@+id/tv_AgendarCitas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="132dp"
        android:layout_marginTop="20dp"
        android:text="@string/AgendarCitas"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ib_1" />

    <TextView
        android:id="@+id/tv_Citas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="152dp"
        android:layout_marginTop="20dp"
        android:text="@string/Citas"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ib_2" />

    <ImageButton
        android:id="@+id/ib_1"
        android:layout_width="176dp"
        android:layout_height="146dp"
        android:layout_marginStart="116dp"
        android:layout_marginTop="124dp"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/Calenario"
        android:contentDescription="Imagen de Calendario"/>

    <ImageButton
        android:id="@+id/ib_2"
        android:layout_width="150dp"
        android:layout_height="144dp"
        android:layout_marginStart="128dp"
        android:layout_marginTop="32dp"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_AgendarCitas"
        app:srcCompat="@mipmap/Nails"
        android:contentDescription="Imagen de uÃ±as"/>



</androidx.constraintlayout.widget.ConstraintLayout>